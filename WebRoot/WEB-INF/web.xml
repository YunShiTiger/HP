<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>WIAPAServlet</servlet-name>
    <servlet-class>WIAPAServlet.WIAPAServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>CmdServlet</servlet-name>
    <servlet-class>WIAPAServlet.CmdServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>MM</servlet-name>
    <servlet-class>WIAPAServlet.MM</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WIAPAServlet</servlet-name>
    <url-pattern>/WIAPAServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CmdServlet</servlet-name>
    <url-pattern>/CmdServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MM</servlet-name>
    <url-pattern>/servlet/MM</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <login-config>
    <auth-method>BASIC</auth-method>
  </login-config>
</web-app>